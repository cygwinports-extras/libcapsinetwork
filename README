  /usr/share/doc/libcapsinetwork-0.3.0/ChangeLog
  /usr/share/doc/libcapsinetwork-0.3.0/INSTALL
  /usr/share/doc/libcapsinetwork-0.3.0/NEWS
  /usr/share/doc/libcapsinetwork-0.3.0/README
  /usr/share/doc/libcapsinetwork-0.3.0/TODO

(libcapsinetwork0)
  /usr/bin/cygcapsinetwork-0.dll

(libcapsinetwork-devel)
  /usr/include/libcapsinetwork/listener.h
  /usr/include/libcapsinetwork/listenport.h
  /usr/include/libcapsinetwork/socket.h
  /usr/lib/libcapsinetwork.a
  /usr/lib/libcapsinetwork.dll.a
